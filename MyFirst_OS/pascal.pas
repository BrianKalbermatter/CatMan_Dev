// ./pascal

program pascal;
var 
    num1 : Integer;
    num2 : Integer;
    resultado : Integer;
    num3 : Byte;
    resultado_de_resultado : Double;
begin
    WriteLn('Me podrias brindar valores para la suma porfavor: ');
    WriteLn('Valor uno: ');
    ReadLn(num1);
    WriteLn('Valor dos: ');
    ReadLn(num2);
    resultado := num1 + num2;
    resultado_de_resultado := resultado + num1 - num3; 
    WriteLn('El resultado es:', resultado);
end.




